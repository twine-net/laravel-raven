{
    "name": "clowdy/laravel-raven",
    "description": "Sentry (Raven) error monitoring for Laravel with send in background",
    "keywords": ["laravel", "sentry", "getsentry", "raven", "monitoring", "error", "logging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan",
            "homepage": "https://github.com/pulkitjalan"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5",
        "illuminate/queue": "~5",
        "illuminate/log": "~5",
        "raven/raven": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9.1"
    },
    "autoload": {
        "psr-4": {
            "Clowdy\\Raven\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
